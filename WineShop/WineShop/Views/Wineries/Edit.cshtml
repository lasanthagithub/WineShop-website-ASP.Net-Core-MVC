@model Winery

<h2>Edit</h2>
<form asp-action="Edit" asp-controller="Wineries">
    <div class="form-horizontal">
        <h4>Winery</h4><hr />
        <div asp-validation-summary="ModelOnly" cass="text-danger"></div>

        <div class="form-group">
            <label asp-for="Name" class="col-md-2 control-label"></label>
            <input asp-for="Name" class="form-control" />
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>

        <div class="form-group">
            <label asp-for="Country" class="col-md-2 control-label"></label>
            <select asp-for="Country" asp-items="@Html.GetEnumSelectList<Country>()" class="form-control "></select>
            <span asp-validation-for="Country" class="text-danger"></span>
        </div>

        <div class="form-group">
            <input type="submit" value="Submit" class="btn btn-default col-md-offset-2" />

        </div>
    </div>
</form>